QComboBox:hover {
    background-color: #dee2e6; /* Couleur au survol */
}

QComboBox:pressed {
    background-color: #ced4da; /* Couleur lorsque le bouton est enfoncé */
}

QComboBox::drop-down {
    width: 25px; /* Largeur de la flèche du bouton déroulant */
    border: none; /* Supprimer la bordure de la flèche */
}

QComboBox::down-arrow {
    image: url(arrow_down.png); /* Remplacer &quot;arrow_down.png&quot; par le chemin vers votre image de flèche personnalisée */
}

/* Styliser la liste déroulante du QComboBox */
QComboBox QAbstractItemView {
    background-color: #e9ecef; /* Couleur de fond de la liste déroulante */
    border: 1px solid #ced4da; /* Bordure de la liste déroulante */
    border-radius: 8px; /* Bords arrondis */
    padding: 8px; /* Espacement interne */
    font-size: 16px; /* Taille de la police */
    font-weight: bold; /* Police en gras */
    min-width: 200px; /* Largeur minimale de la liste déroulante */
}

QComboBox QAbstractItemView::item {
